{
  "name": "ts-base-project",
  "version": "1.0.0",
  "description": "Strict TypeScript template using yarn / Github Actions / ES6 / TypeScript / ESLint / Prettier",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "clean": "del-cli -f ./dist ./types",
    "lint": "yarn run eslint --fix --ext .ts src --cache src --cache-location node_modules/.cache/.eslintcache",
    "prebuild": "yarn clean && yarn run lint",
    "build": "yarn run tsc",
    "prepublish": "yarn build",
    "test": "echo test",
    "dev:test": "echo test --watch",
    "dev:build": "tsc --watch",
    "start": "node ./dist/index.js"
  },
  "author": "MathieuGermain",
  "license": "MIT",
  "homepage": "https://github.com/MathieuGermain/ts-base-project#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathieuGermain/ts-base-project.git"
  },
  "bugs": {
    "url": "https://github.com/MathieuGermain/ts-base-project/issues"
  },
  "dependencies": {

  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint": "^8.23.0",
    "prettier": "2.7.1",
    "del-cli": "^5.0.0",
    "typescript": "^4.8.3"
  }
}
